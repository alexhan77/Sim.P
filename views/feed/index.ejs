
<div class="container center-align">
<h1>Sim.p's Feed Page </h1>


<div class="row">
<% feed.forEach(f => { %>
    <div class="col s12 m4">
        <div class="card">
            <div class="card-image">
                <img src="<%=f.pic%>"id="profilepic">
            </div>
            <span class="card-title">Post Created By: <%=f.user.username %> </span>
            
            <div class="card-content">
                <p>Caption: <%=f.content%></p>
            </div>
            <div class="card-action">
                <form method="POST" action="/faves/results">
                    <input type="hidden" name="userId" value="<%= f.userId %>">
                    <input type="hidden" name="feedId" value="<%= f.id %>">
                    <button type="submit">ðŸ’œ</button>
                </form> 
            </div>
        </div>
    </div>
    <% }) %>
</div>

<a href="/profile/user" class="waves-effect waves-light btn"><i class="material-icons right">cloud</i> Back to ur Sim.p profile</a>
</div>